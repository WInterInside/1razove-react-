\Users\\Winter\\Desktop\\projects\\1razove-react-\\1razove\\src\\pages\\ErrorPage.js",[],"C:\\Users\\Winter\\Desktop\\projects\\1razove-react-\\1razove\\src\\pages\\BrandPage.js",[],"C:\\Users\\Winter\\Desktop\\projects\\1razove-react-\\1razove\\src\\components\\brands\\Brands.js",["145","146"],"import React from \"react\"\r\nimport { useState } from 'react';\r\nimport \"./Brands.scss\";\r\n\r\n\r\nexport default function Brands({data}) {\r\n\r\n\r\n  const [count, setCount] = useState(window.innerWidth <= 950 ? 4:100);\r\n  const [name, setName] = useState(\"Показати більше\");\r\n\r\n\r\n  function toggleBrands() {\r\n    if (count <= 4) {\r\n      setCount(count + 100);\r\n      setName(\"Згорнути\");\r\n    } else {\r\n      setCount(count - 100);\r\n      setName(\"Показати більше\");\r\n    }\r\n  }\r\n\r\n  return (\r\n    <section className=\"brands\">\r\n      <a className=\"brands__link\" name=\"brands\"></a>\r\n      <div className=\"container\">\r\n        <h2 className=\"brands__heading\">{data.title}</h2>\r\n        <ul className=\"brands__list\">\r\n          {\r\n            data.brands.slice(0, count).map((value,index) => {\r\n              return <li key={index} className=\"brands__item\">\r\n                <a className=\"brands__link\" href={`/brand/${value.url}`}>\r\n                  <div className=\"brands__wrapper\">\r\n                    {/* <h2 className=\"brands__name visually-hidden\">Maxtec</h2> */}\r\n                    <img className=\"brands__img brands__img--maxtec\" src={value.img} alt=\"maxtec logotype\" width=\"176\" height=\"59\" />\r\n                    <p className=\"brands__description brands__description--maxtec\">{value.text}</p>\r\n                  </div>\r\n                </a>\r\n              </li>\r\n            })\r\n          }\r\n        </ul>\r\n        <button className={`brands__button button button--more button--on ${data.brands.length <= 4 ? 'hidden' : ''}`} type=\"button\" name=\"button\" onClick={() => toggleBrands()}>\r\n          <span className=\"button__text\">{name}</span>\r\n        </button>\r\n      </div>\r\n    </section>\r\n  )\r\n}\r\n","C:\\Users\\Winter\\Desktop\\projects\\1razove-react-\\1razove\\src\\components\\slider\\Slider.js",["147","148"],"import React, {useEffect, useState} from \"react\";\r\n\r\nimport {tns} from 'tiny-slider/src/tiny-slider';\r\nimport \"./Slider.scss\";\r\nimport Overlay from '../overlay/Overlay';\r\n\r\nlet initialized = false;\r\nlet imgIndex = 0;\r\nexport default function Slider({data}) {\r\n  let [selectedImg, setSelectedImage] = useState('');\r\n  let [showPopup, setShowPopup] = useState(false);\r\n\r\n  function openOverlay(url, index){\r\n    setSelectedImage(url);\r\n    imgIndex = index;\r\n    setShowPopup(true);\r\n  }\r\n\r\n  function closePopup(){\r\n    setShowPopup(false);\r\n  }\r\n\r\n  function moveSlides(e, increment){\r\n    e.stopPropagation();\r\n    e.nativeEvent.stopImmediatePropagation();\r\n    if(!isAvailable(increment))\r\n      return;\r\n\r\n    imgIndex += increment;\r\n    setSelectedImage(data.images[imgIndex].url, imgIndex);\r\n  }\r\n\r\n  function isAvailable(increment){\r\n    return ((imgIndex + increment) < data.images.length) && ((imgIndex + increment) >= 0);\r\n  }\r\n\r\n  useEffect (() => {\r\n    if(initialized)\r\n      return;\r\n\r\n    setTimeout (() => {\r\n      var slider = tns({\r\n        container: '.product__slider',\r\n        items: 2,\r\n        autoWidth: true,\r\n        gutter: 10,\r\n        controls: true,\r\n        nav: false,\r\n        autoplay: false,\r\n        loop: false,\r\n        mouseDrag: true,\r\n        arrowKeys: true,\r\n        controlsContainer: '.product__toggls',\r\n        prevButton: '.product__arrow--prev',\r\n        nextButton: '.product__arrow--next',\r\n        responsive: {\r\n          900: {\r\n            items: 4,\r\n            gutter: 15\r\n          }\r\n        }\r\n      });\r\n      initialized = true;\r\n    }, 200)\r\n  });\r\n\r\n  return (\r\n    <div className=\"product__slides\">\r\n      <div className=\"product__wrapper container\">\r\n        <h2 className=\"product__heading product__heading--photos\">{data.imgTitle}</h2>\r\n        <div className=\"product__toggls\">\r\n          <div className=\"product__arrow product__arrow--prev\">←</div>\r\n          <div className=\"product__arrow product__arrow--next\">→</div>\r\n        </div>\r\n      </div>\r\n      <div className=\"container container--slider\">\r\n        <div className=\"product__slider\">\r\n          {\r\n            data.images.map((value,index) => {\r\n              return <div key={index} className=\"product__slide\" onClick={() => openOverlay(value.url, index)}>\r\n                <picture>\r\n                  <source type=\"image/webp\" srcSet={value.url} />\r\n                  <img className=\"product__slideimg\" src={value.url} alt={value.title} />\r\n                </picture>\r\n              </div>\r\n            })\r\n          }\r\n        </div>\r\n      </div>\r\n      {\r\n        showPopup && <Overlay closePopup={() => closePopup()}>\r\n          <div className=\"slider__popup\">\r\n            <div className=\"slider__popup-left\" onClick={(e) => moveSlides(e, -1)}></div>\r\n            <div className=\"slider__popup-right\" onClick={(e) => moveSlides(e, 1)}></div>\r\n            <div className=\"wrapper__popup\">\r\n              <picture>\r\n                <img className=\"product__slideimg\" src={selectedImg} />\r\n              </picture>\r\n              <div className=\"close\" onClick={() => closePopup()}></div>\r\n            </div>\r\n          </div>\r\n\r\n        </Overlay>\r\n      }\r\n    </div>\r\n  )\r\n}\r\n","C:\\Users\\Winter\\Desktop\\projects\\1razove-react-\\1razove\\src\\components\\brandDescription\\BrandDescription.js",["149"],"import React, {useState} from \"react\"\r\nimport \"./Brand.scss\";\r\nimport Overlay from '../overlay/Overlay';\r\nimport Form from '../form/Form';\r\n\r\nexport default function BrandDescription({data}) {\r\n  let [showPopup, setShowPopup] = useState(false);\r\n\r\n  function closePopup(){\r\n    setShowPopup(false);\r\n  }\r\n\r\n  function openPopup(){\r\n    setShowPopup(true);\r\n  }\r\n\r\n  return (\r\n    <section className=\"brand__discription\">\r\n      <div className=\"container\">\r\n        <div className=\"brand__wrapper\">\r\n          <div className=\"brand__logotype brand__logotype--desctop\">\r\n          <picture>\r\n            <img className=\"brand__img\" src={data.brandImage} alt=\"logotype\" width=\"277\" height=\"92\" />\r\n          </picture>\r\n          </div>\r\n          <div className=\"brand__wrapper brand__wrapper--column\">\r\n            <h1 className=\"brand__heading\">{data.title}</h1>\r\n            <div className=\"brand__logotype brand__logotype--mobile\">\r\n              <picture>\r\n                <img className=\"brand__img\" src={data.brandImage} alt=\"logotype\" width=\"277\" height=\"92\" />\r\n              </picture>\r\n            </div>\r\n            <p className=\"brand__text brand__text--discription\">{data.description}</p>\r\n            <div className=\"brand__sphere\">\r\n              <h2 className=\"brand__heading brand__heading--colored brand__heading--h2\">{data.blueText}</h2>\r\n              <p className=\"brand__text brand__text--sphere\">{data.text}</p>\r\n            </div>\r\n            <a className=\"brand__link button button--price\" onClick={() => openPopup()}>{data.btnText}</a>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      { \r\n        showPopup && <Overlay closePopup={() => closePopup()}>\r\n          <Form closePopup={() => closePopup()}/>\r\n        </Overlay>\r\n      }\r\n\r\n    </section>\r\n  )\r\n}\r\n","C:\\Users\\Winter\\Desktop\\projects\\1razove-react-\\1razove\\src\\components\\contacts\\Contacts.js",["150","151","152","153"],"C:\\Users\\Winter\\Desktop\\projects\\1razove-react-\\1razove\\src